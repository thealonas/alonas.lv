---
const thx = [
  [
    {
      url: 'https://1password.com/',
      name: '1Password',
      logo: 'data:image/svg+xml;base64,',
      class: 'w-full'
    },
    {
      url: 'https://www.atlassian.com/',
      name: 'Atlassian',
      logo: 'https://wac-cdn.atlassian.com/dam/jcr:616e6748-ad8c-48d9-ae93-e49019ed5259/Atlassian-horizontal-blue-rgb.svg',
      class: 'w-full'
    },
    {
      url: 'https://jb.gg/OpenSourceSupport',
      name: 'JebBrains',
      logo: 'https://resources.jetbrains.com/storage/products/company/brand/logos/jb_beam.svg',
      class: 'w-80'
    },
    {
      url: 'https://www.browserstack.com/',
      name: 'BrowserStack',
      logo: 'data:image/svg+xml;base64,',
      class: 'w-full'
    }
  ],
  [
    {
      url: 'https://sentry.io/for/good/',
      name: 'Sentry',
      logo: 'data:image/svg+xml;base64,',
      class: 'w-full'
    },
    {
      url: 'https://www.algolia.com/',
      name: 'Algolia',
      logo: 'https://docsearch.algolia.com/img/Algolia-logo-blue.svg',
      class: 'w-full'
    },
    {
      url: 'https://www.macstadium.com/',
      name: 'MacStadium',
      logo: 'data:image/svg+xml;base64,',
      class: 'w-full'
    },
    {
      url: 'https://tuta.com/',
      name: 'Tuta',
      logo: 'data:image/svg+xml;base64,',
      class: 'w-full'
    }
  ]
];
---

<section class="py-16 sm:py-20">
  <div class="mx-auto max-w-2xl px-4 sm:px-6 lg:max-w-7xl lg:px-8">
    <div class="flex flex-col gap-8 lg:gap-12">
      <h2 class="text-3xl font-medium tracking-tight sm:text-4xl" id="thanks">🥰 Thanks also to these companies</h2>
      <div class="grid gap-8 lg:grid-cols-2">
        {
          thx.map((column, index) => (
            <div class:list={['space-y-8', index === 1 && 'lg:mt-16']}>
              {column.map((thx) => (
                <div class="bg-primary-500/10 dark:bg-primary-400/10 flex flex-col gap-12 rounded-3xl px-6 py-8">
                  <div class="flex flex-col gap-4">
                    <a href={thx.url} target="_blank" rel="noopener"><img src={thx.logo} alt={thx.name} class={thx.class}/></a>
                  </div>
                </div>
              ))}
            </div>
          ))
        }
      </div>
    </div>
  </div>
</section>
